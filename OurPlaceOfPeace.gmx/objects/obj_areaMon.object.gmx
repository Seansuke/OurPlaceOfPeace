var closeDistance = distance_to_object(closestPlayer);
if(closeDistance &gt; 320) {
    exit;
}

if((closestPlayer).x &gt; x) {
    if(areaPlaceClear(x + v_speed,y)) {
        x += v_speed;
        v_dir = DIR_RIGHT;
    }
}
else if((closestPlayer).x &lt; x) {
    if(areaPlaceClear(x - v_speed, y)) {
        x -= v_speed;
        v_dir = DIR_LEFT;
    }
}
if((closestPlayer).y &gt; y) {
    if(areaPlaceClear(x,y + v_speed)) {
        y += v_speed;
    }
}
else if((closestPlayer).y &lt; y) {
    if(areaPlaceClear(x,y - v_speed)) {
        y -= v_speed;
    }
}

